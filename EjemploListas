import java.util.List;
import java.util.ArrayList;

public class EjemploListas{
  
   public static void main(String[] args){
     
     Peliculas p1 = new Peliculas();
     Peliculas p2 = new Peliculas();
     
     List<Peliculas> lista;
     lista = new ArrayList<Peliculas>(); //Puede no escribirse la palabra Peliculas en esta instruccion
     lista.add(p1); //Se a√±aden los objetos al arraylist
     lista.add(p2);
   }
}
